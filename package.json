{
  "name": "effect-tree",
  "description": "A strict tree datatype that wants to do everything.",
  "version": "1.0.0",
  "type": "module",
  "author": "Ran Eilam <ran@haag59.com> (https://middle-ages.github.io/)",
  "homepage": "https://github.com/middle-ages/effect-tree",
  "bugs": "https://github.com/middle-ages/effect-tree/issues",
  "license": "ISC",
  "scripts": {
    "check": "dev/check",
    "repl": "tsx --import=./dev/repl.ts",
    "clean": "dev/clean",
    "lint": "dev/lint --cache",
    "lint:fix": "dev/lint-fix --cache",
    "lint:clean": "dev/lint",
    "lint::clean:fix": "dev/lint-fix",
    "test": "dev/test-run",
    "test:watch": "dev/test-watch",
    "test:examples": "dev/test-examples",
    "coverage": "dev/coverage",
    "typecheck": "dev/typecheck",
    "circular": "dev/circular",
    "build": "dev/build",
    "build:docs": "dev/build-docs",
    "build:all": "dev/build-all",
    "publish:docs": "dev/publish-docs"
  },
  "devDependencies": {
    "@effect/vitest": "^0.25.1",
    "@eslint/js": "^9.36.0",
    "@fast-check/vitest": "^0.2.2",
    "@tsconfig/node24": "^24.0.1",
    "@tsconfig/strictest": "^2.0.6",
    "@vitest/coverage-v8": "^3.2.4",
    "dependency-cruiser": "^17.0.1",
    "effect-ts-laws": "^1.3.18",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "fast-check": "^4.3.0",
    "jiti": "^2.6.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@effect/typeclass": "^0.36.0",
    "effect": "^3.17.14",
    "effect-ts-folds": "^1.0.30",
    "tty-strings": "^1.5.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "import": "./index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./src/*.d.ts",
        "import": "./src/*.js"
      }
    }
  },
  "imports": {
    "#util": {
      "types": "./src/util.d.ts",
      "import": "./src/util.js"
    },
    "#util/*": {
      "types": "./src/util/*.d.ts",
      "import": "./src/util/*.js"
    },
    "#tree": {
      "types": "./src/tree.d.ts",
      "import": "./src/tree.js"
    },
    "#treeF": {
      "types": "./src/treeF.d.ts",
      "import": "./src/treeF.js"
    },
    "#ops": {
      "types": "./src/ops.d.ts",
      "import": "./src/ops.js"
    },
    "#zipper": {
      "types": "./src/zipper.d.ts",
      "import": "./src/zipper.js"
    },
    "#ops/*": {
      "types": "./src/ops/*.d.ts",
      "import": "./src/ops/*.js"
    },
    "#codec": {
      "types": "./src/codec.d.ts",
      "import": "./src/codec.js"
    },
    "#folds": {
      "types": "./src/folds.d.ts",
      "import": "./src/folds.js"
    },
    "#unfolds": {
      "types": "./src/unfolds.d.ts",
      "import": "./src/unfolds.js"
    },
    "#draw": {
      "types": "./src/draw.d.ts",
      "import": "./src/draw.js"
    },
    "#draw/*": {
      "types": "./src/draw/*.d.ts",
      "import": "./src/draw/*.js"
    },
    "#arbitrary": {
      "types": "./src/arbitrary.d.ts",
      "import": "./src/arbitrary.js"
    },
    "#arbitrary/*": {
      "types": "./src/arbitrary/*.d.ts",
      "import": "./src/arbitrary/*.js"
    },
    "#test": {
      "types": "./src/test.d.ts",
      "import": "./src/test.js"
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/middle-ages/effect-tree"
  },
  "keywords": [
    "effect",
    "tree",
    "data structures"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "bracketSameLine": true,
    "printWidth": 80,
    "endOfLine": "lf"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
