{
  "name": "effect-tree",
  "description": "A strict tree datatype that wants to do everything.",
  "version": "1.0.14",
  "type": "module",
  "author": "Ran Eilam <ran@haag59.com> (https://middle-ages.github.io/)",
  "homepage": "https://github.com/middle-ages/effect-tree",
  "bugs": "https://github.com/middle-ages/effect-tree/issues",
  "license": "ISC",
  "scripts": {
    "check": "dev/check",
    "repl": "tsx --import=./dev/repl.ts",
    "clean": "dev/clean",
    "extract:inline:examples": "dev/extract-inline-examples",
    "lint": "dev/lint --cache",
    "lint:fix": "dev/lint-fix --cache",
    "lint:clean": "dev/lint",
    "lint::clean:fix": "dev/lint-fix",
    "test": "dev/test-run",
    "test:watch": "dev/test-watch",
    "test:project:examples": "dev/test-project-examples",
    "test:markdown:examples": "dev/test-markdown-examples",
    "test:inline:examples": "dev/test-inline-examples",
    "test:examples": "dev/test-examples",
    "run:example": "dev/run-example",
    "coverage": "dev/coverage",
    "typecheck": "dev/typecheck",
    "circular": "dev/circular",
    "walk": "depwalker --depth 3 --format tree --tsconfig ./config/tsconfig.main.json",
    "build": "dev/build",
    "build:docs": "dev/build-docs",
    "publish:docs": "dev/publish-docs",
    "npm": "dev/publish"
  },
  "devDependencies": {
    "@effect/vitest": "^0.25.1",
    "@eslint/js": "^9.36.0",
    "@fast-check/vitest": "^0.2.2",
    "@tsconfig/node24": "^24.0.1",
    "@tsconfig/strictest": "^2.0.6",
    "@vitest/coverage-v8": "^3.2.4",
    "dependency-cruiser": "^17.0.1",
    "depwalker": "^0.1.3",
    "effect-ts-laws": "^1.3.18",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "fast-check": "^4.3.0",
    "jiti": "^2.6.0",
    "prettier": "^3.6.2",
    "ts-morph": "^27.0.0",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@effect/typeclass": "^0.36.0",
    "effect": "^3.17.14",
    "effect-ts-folds": "^1.0.30",
    "tty-strings": "^1.5.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "import": "./index.js"
      }
    }
  },
  "imports": {
    "#util": {
      "types": "./src/util.d.ts",
      "import": "./src/util.js"
    },
    "#Array": {
      "types": "./src/util/Array.d.ts",
      "import": "./src/util/Array.js"
    },
    "#Function": {
      "types": "./src/util/Function.d.ts",
      "import": "./src/util/Function.js"
    },
    "#Number": {
      "types": "./src/util/Number.d.ts",
      "import": "./src/util/Number.js"
    },
    "#Order": {
      "types": "./src/util/Order.d.ts",
      "import": "./src/util/Order.js"
    },
    "#Pair": {
      "types": "./src/util/Pair.d.ts",
      "import": "./src/util/Pair.js"
    },
    "#Record": {
      "types": "./src/util/Record.d.ts",
      "import": "./src/util/Record.js"
    },
    "#String": {
      "types": "./src/util/String.d.ts",
      "import": "./src/util/String.js"
    },
    "#These": {
      "types": "./src/util/These.d.ts",
      "import": "./src/util/These.js"
    },
    "#tree": {
      "types": "./src/tree.d.ts",
      "import": "./src/tree.js"
    },
    "#treeF": {
      "types": "./src/treeF.d.ts",
      "import": "./src/treeF.js"
    },
    "#ops": {
      "types": "./src/ops.d.ts",
      "import": "./src/ops.js"
    },
    "#zipper": {
      "types": "./src/zipper.d.ts",
      "import": "./src/zipper.js"
    },
    "#codec": {
      "types": "./src/codec.d.ts",
      "import": "./src/codec.js"
    },
    "#folds": {
      "types": "./src/folds.d.ts",
      "import": "./src/folds.js"
    },
    "#unfolds": {
      "types": "./src/unfolds.d.ts",
      "import": "./src/unfolds.js"
    },
    "#draw": {
      "types": "./src/draw.d.ts",
      "import": "./src/draw.js"
    },
    "#arbitrary": {
      "types": "./src/arbitrary.d.ts",
      "import": "./src/arbitrary.js"
    },
    "#test": {
      "types": "./src/test.d.ts",
      "import": "./src/test.js"
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/middle-ages/effect-tree"
  },
  "keywords": [
    "effect",
    "tree",
    "data structures"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "bracketSameLine": true,
    "printWidth": 80,
    "endOfLine": "lf"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
